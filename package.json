{
  "name": "@imrim12/nuxt-template",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.mjs build",
    "lint": "eslint . -c .eslintrc.cjs --fix --ext .ts,.js,.cjs,.vue,.tsx,.jsx",
    "build:icons": "tsx plugins/iconify/build-icons.ts",
    "postinstall": "nuxt prepare && npm run build:icons ",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "supabase:start": "supabase start --exclude realtime,storage-api,imgproxy,inbucket,edge-runtime,logflare,vector,supavisor",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:restart": "supabase stop && supabase start --exclude realtime,storage-api,imgproxy,inbucket,edge-runtime,logflare,vector,supavisor",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase migration up",
    "supabase:link": "supabase link",
    "supabase:generate-types": "supabase gen types typescript --local --schema public > server/types/supabase.ts",
    "supabase:generate-migration": "supabase db diff | supabase migration new",
    "supabase:generate-seed": "supabase db dump --data-only --local --schema public -f supabase/seed.sql",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull"
  },
