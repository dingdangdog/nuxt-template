{
  "name": "@imrim12/nuxt-template",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.4.0",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "jiti ./scripts/serve.ts",
    "build": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.mjs build",
    "postbuild": "jiti ./scripts/postbuild.ts",
    "lint": "eslint . --fix",
    "postinstall": "pnpm prepare && puppeteer browsers install",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "test": "vitest run",
    "stripe:seed": "jiti ./scripts/generate-stripe.ts --esmResolve",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit generate && drizzle-kit migrate",
    "db:seed": "jiti ./server/db/seeds/index.seed.ts",
    "db:clear": "rimraf ./docker/postgres",
    "db:start": "docker compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "pnpm db:stop && pnpm db:clear && pnpm db:start --force-recreate",
    "gettext:extract": "vue-gettext-extract",
    "gettext:compile": "vue-gettext-compile"
  },
  "dependencies": {
    "@imrim12/base": "workspace:*"
  },
  "overrides": {
    "@tiptap/core": "^2",
    "@types/video.js": "^7",
    "postcss": "^8",
    "stylelint-order": "6.0.3"
  },
  "resolutions": {
    "@tiptap/core": "^2",
    "@types/video.js": "^7",
    "postcss": "^8",
    "stylelint-order": "6.0.3"
  }
}
