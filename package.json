{
  "name": "@imrim12/nuxt-template",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "jiti ./scripts/serve.ts",
    "build": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.mjs build",
    "postbuild": "jiti ./scripts/postbuild.ts",
    "lint": "eslint . --fix",
    "build:icons": "tsx app/plugins/iconify/build-icons.ts",
    "postinstall": "nuxt prepare && puppeteer browsers install",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "test": "vitest run",
    "stripe:seed": "jiti ./scripts/generate-stripe.ts --esmResolve",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit generate && drizzle-kit migrate",
    "db:seed": "jiti ./server/db/seeds/index.seed.ts",
    "db:clear": "rimraf ./docker/postgres",
    "db:start": "docker compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "pnpm db:stop && pnpm db:clear && pnpm db:start --force-recreate",
    "gettext:extract": "vue-gettext-extract",
    "gettext:compile": "vue-gettext-compile"
  },
  "dependencies": {
    "@imrim12/base": "workspace:*"
  },
  "devDependencies": {
    "@antfu/utils": "0.7.7",
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/s3-request-presigner": "^3.637.0",
    "@fullcalendar/core": "6.1.11",
    "@fullcalendar/daygrid": "6.1.11",
    "@fullcalendar/interaction": "6.1.11",
    "@fullcalendar/list": "6.1.11",
    "@fullcalendar/timegrid": "6.1.11",
    "@fullcalendar/vue3": "6.1.11",
    "@iconify-json/mdi": "1.1.66",
    "@iconify-json/ri": "1.1.20",
    "@iconify/tools": "4.0.4",
    "@iconify/utils": "2.1.23",
    "@iconify/vue": "4.1.1",
    "@imrim12/eslint-config": "workspace:*",
    "@intlify/unplugin-vue-i18n": "1.6.0",
    "@ngneat/falso": "^7.2.0",
    "@nuxt/kit": "^3.13.2",
    "@nuxt/schema": "^3.13.2",
    "@nuxtjs/device": "3.1.1",
    "@pinia/nuxt": "^0.5.1",
    "@sidebase/nuxt-auth": "^0.8.0",
    "@stylistic/stylelint-config": "1.0.1",
    "@stylistic/stylelint-plugin": "2.1.1",
    "@tiptap/extension-character-count": "2.3.0",
    "@tiptap/extension-placeholder": "2.3.0",
    "@tiptap/extension-subscript": "2.3.0",
    "@tiptap/extension-superscript": "2.3.0",
    "@tiptap/extension-underline": "2.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/mapbox-gl": "3.1.0",
    "@types/node": "20.12.7",
    "@types/pdfmake": "^0.2.9",
    "@types/pg": "^8.11.6",
    "@types/webfontloader": "1.6.38",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "@videojs-player/vue": "1.0.0",
    "@vueuse/nuxt": "^10.11.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-case-police": "0.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regex": "1.10.0",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-unicorn": "51.0.1",
    "eslint-plugin-vue": "9.25.0",
    "execa": "^9.3.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "postcss-html": "1.6.0",
    "postcss-scss": "4.0.9",
    "postgres": "^3.4.4",
    "puppeteer": "^23.4.1",
    "rimraf": "^6.0.1",
    "sass": "1.75.0",
    "shikiji": "0.10.2",
    "stylelint": "16.2.1",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-use-logical-spec": "5.0.1",
    "tsx": "4.7.3",
    "type-fest": "4.17.0",
    "typescript": "5.4.5",
    "video.js": "8.6.0",
    "vite": "5.2.10",
    "vitest": "^2.1.1",
    "vue": "^3.5.8",
    "vue-shepherd": "3.0.0"
  },
  "overrides": {
    "@tiptap/core": "^2",
    "@types/video.js": "^7",
    "postcss": "^8",
    "pug": "^3",
    "stylelint-order": "6.0.3"
  },
  "resolutions": {
    "@tiptap/core": "^2",
    "@types/video.js": "^7",
    "postcss": "^8",
    "pug": "^3",
    "stylelint-order": "6.0.3"
  }
}
